RWStructuredBuffer<float> block : register(u0, space0);
Texture2D<float> tex : register(t1, space0);
SamplerState samps[16] : register(s0, space1);
[numthreads(256,1,1)]
void main(uint id : SV_DISPATCHTHREADID)
{
    block[id] = tex.SampleLevel(samps[3], float2((id + 0.5) / 1024.0, 1.0 / 1024.0), 0);
}